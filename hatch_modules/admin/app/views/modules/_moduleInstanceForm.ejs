<input type="hidden" name="name" value="<%- module.name %>" />

<div class="control-group">
    <input type="hidden" name="contract" id="contractValue" value='<%- JSON.stringify(module.contract || {}) %>' />

    <div id="contract" style="position : relative; width : 100%; height: 120px; font-family : 'Courier New', Courier, fixed-width;" rows="10"><%- JSON.stringify(module.contract || {}, undefined, 4) %></div>
    <div class="clearfix"></div>
</div>